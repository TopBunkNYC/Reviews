{
  "name": "reviews",
  "version": "1.1.0",
  "description": "Reviews module for TopBunk",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "server-dev": "nodemon server/index.js",
    "react-dev": "webpack -d --watch",
    "react-prod": "webpack -p",
    "test": "jest",
    "build-csv": "node build-database/build-csv.js",
    "load-csv": "psql topbunk < ./database-pg/pgSchema.sql && time psql topbunk < build-database/copy-pg.sql",
    "seed-mySQL": "mysql -u root -p < database-mysql/mysqlSchema.sql",
    "postseed-mySQL": "mysql -u root -p < database-mysql/sampleData.sql",
    "seed-pg-in-memory": "psql topbunk < ./database-pg/pgSchema.sql && node ./build-database/build.js",
    "start-aws": "pm2 start server/index.js",
    "listpm2": "pm2 list",
    "pm2stop": "pm2 stop 0",
    "connect": "ssh -i \"topbunkreviews.pem\" ubuntu@ec2-PLACEHOLDER.us-east-2.compute.amazonaws.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TopBunkNYC/Reviews.git"
  },
