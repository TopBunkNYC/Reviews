    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "performance-now": "^2.1.0",
    "puppeteer": "^1.10.0",
    "rest": "^2.0.0",
    "style-loader": "^0.23.1",
